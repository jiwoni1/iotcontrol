{
  "rewrites": [
    {
      "source": "/events/:path*",
      "destination": "https://192.168.0.90:3009/events/:path*"
    },
    {
      "source": "/data/:path*",
      "destination": "https://192.168.0.90:3009/data/:path*"
    },
    {
      "source": "/control/:path*",
      "destination": "https://192.168.0.90:3008/control/:path*"
    },
    {
      "source": "/spotcontrol/:path*",
      "destination": "https://192.168.0.90:3007/spotcontrol/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },

    {
      "source": "/.well-known/assetlinks.json",
      "destination": "/.well-known/assetlinks.json"
    }
  ]
}
