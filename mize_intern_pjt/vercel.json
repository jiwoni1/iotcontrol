{
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://192.168.0.90:3002/api"
    },
    {
      "source": "/controller/:path*",
      "destination": "https://192.168.0.90:3008/control/:path*"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    },

    {
      "source": "/.well-known/:path*",
      "destination": "/.well-known/:path*"
    }
  ]
}
